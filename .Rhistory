##### eccDNA with rRNA on chromosome
raw02 <-
read.delim("concat_all", header=F, sep="\t") %>% filter(V8 %nin% c("x1", "x2", "x3")) %>%
group_by(V8, V2) %>%
mutate(molecule=V1, spid=V8, eccid=V2, speccid=paste(spid, eccid, sep = "_"), start=V3, end=V4, strand="forward",
class1=V6, class2=V7, class3 = str_remove(class2, "/.+") %>% str_replace(., ".*rRNA.*", "rRNA"),
from=V3, to=V4, orientation=ifelse(V5=="+", 1, 0), .keep = "none") %>%
ungroup() %>%
select(-c(V2, V8)) %>%
mutate(sp = str_remove(spid, "[0-9]"), id = str_remove(spid, "[A-z]")) %>%
group_by(sp, eccid) %>%
mutate(serial=seq(n()))
